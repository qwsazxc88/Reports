<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="BugReport">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Summary" not-null="true"/>
    <property name="UserRole" not-null="true"/>
    <property name="Description" not-null="true"/>
    <property name="Browser" not-null="true"/>
    <property name="BrowserVersion" not-null="true"/>
    <property name="Guid" not-null="true"/>
    <many-to-one name="User" class="User" column="UserId" foreign-key="FK_BugReport_Users" not-null="true"/>
  </class>
</hibernate-mapping>
