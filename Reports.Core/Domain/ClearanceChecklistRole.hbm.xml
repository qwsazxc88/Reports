<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="ClearanceChecklistRole" table="[ClearanceChecklistRole]">
    
    <id name="Id">
      <generator class="native" />
    </id>
    
    <version name="Version" column= "Version"/>

    <property name="Code" length="32" not-null="false" />
    
    <property name="Description" length="256" not-null="false"/>

    <property name="DaysForApproval" not-null="false"/>
  
  </class>
</hibernate-mapping>