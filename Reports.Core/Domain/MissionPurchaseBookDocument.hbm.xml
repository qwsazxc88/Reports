	<property name="Sum"  not-null="true"/>
	<many-to-one name="Editor" class="User" column="EditorId" foreign-key="FK_MissionPurchaseBookDocument_EditorUser" index="IX_MissionPurchaseBookDocument_EditorUser"  not-null="true"/>

