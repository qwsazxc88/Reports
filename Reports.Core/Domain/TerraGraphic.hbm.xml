<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="TerraGraphic" dynamic-update="true" table="TerraGraphic">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="UserId" not-null="true"/>
    <property name="Day" not-null="true"/>
    <property name="Hours" not-null="false"/>
    <property name="PointId" not-null="false"/>
    <!--<property name="IsCreditAvailable" not-null="false"/>-->
    <property name="IsCreditAvailable" not-null="false"/>
  </class>
</hibernate-mapping>