<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="ClearanceChecklistApproval">
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>
    <many-to-one name="ClearanceChecklist" class="ClearanceChecklist" column="ClearanceChecklistId" foreign-key="FK_ClearanceChecklistApproval_ClearanceChecklist" index="IX_ClearanceChecklistApproval_ClearanceChecklist_Id"  not-null="true"/>
    <many-to-one name="ClearanceChecklistDepartment" class="ClearanceChecklistDepartment" column="ClearanceChecklistDepartmentId" foreign-key="FK_ClearanceChecklistApproval_ClearanceChecklistDepartment" index="IX_ClearanceChecklistApproval_ClearanceChecklistDepartment_Id"  not-null="true"/>
    <many-to-one name="ApprovedBy" class="User" column="ApprovedById" foreign-key="FK_ClearanceChecklistApproval_ApprovedBy" index="IX_ClearanceChecklistApproval_ApprovedBy_Id"  not-null="false"/>
    <property name="ApprovalDate" not-null="false"/>
  </class>
</hibernate-mapping>
