<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="Disability" dynamic-update="true" table="Disability">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <property name="CertificateSeries" length="10" not-null="false"/>
    <property name="CertificateNumber" length="10" not-null="false"/>
    <property name="CertificateDateOfIssue" not-null="false"/>
    <property name="DisabilityDegree" length="2" not-null="false"/>
    <property name="CertificateExpirationDate" not-null="false"/>

  </class>
</hibernate-mapping>