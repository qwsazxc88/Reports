<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="Certification" dynamic-update="true" table="Certification">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <property name="CertificationDate" not-null="false"/>
    <property name="CertificateNumber" length="20" not-null="false"/>
    <property name="CertificateDateOfIssue" not-null="false"/>
    <property name="InitiatingOrder" length="20" not-null="false"/>
    
  </class>
</hibernate-mapping>