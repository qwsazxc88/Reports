<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="FamilyMember" dynamic-update="true" table="FamilyMember">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <many-to-one name="Relationship" class="FamilyRelationship" column="RelationshipId" foreign-key="FK_FamilyMember_Relationship" index="FamilyMember_Relationship"  not-null="false"/>
    <property name="Name" length="50" not-null="false"/>
    <property name="PassportData" length="150" not-null="false"/>
    <property name="DateOfBirth" not-null="false"/>
    <property name="PlaceOfBirth" length="150" not-null="false"/>
    <property name="WorksAt" length="50" not-null="false"/>
    <property name="Contacts" length="100" not-null="false"/>
    
  </class>
</hibernate-mapping>