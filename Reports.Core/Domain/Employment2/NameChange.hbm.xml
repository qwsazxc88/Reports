<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="NameChange" dynamic-update="true" table="NameChange">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <property name="PreviousName" length="50" not-null="false"/>
    <property name="Date" not-null="false"/>

    <property name="Place" length="50" not-null="false"/>
    <property name="Reason" length="50" not-null="false"/>

  </class>
</hibernate-mapping>