<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="ForeignLanguage" dynamic-update="true" table="ForeignLanguage">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <property name="LanguageName" length="50" not-null="false"/>
    <property name="Level" length="20" not-null="false"/>
    
  </class>
</hibernate-mapping>