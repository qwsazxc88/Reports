<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="Automobile" dynamic-update="true" table="Automobile">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <property name="Make" length="100" not-null="false"/>
    <property name="LicensePlateNumber" length="10" not-null="false"/>
    
  </class>
</hibernate-mapping>