<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Reports.Core.Domain" assembly="Reports.Core">
  <class name="Training" dynamic-update="true" table="Training">
    
    <id name="Id">
      <generator class="native" />
    </id>
    <version name="Version" column= "Version"/>

    <property name="CertificateIssuedBy" length="50" not-null="false"/>
    <property name="Series" length="10" not-null="false"/>
    <property name="Number" length="10" not-null="false"/>
    <property name="BeginningDate" not-null="false"/>
    <property name="EndDate" not-null="false"/>
    <property name="Speciality" length="50" not-null="false"/>
    <property name="LocationEI" length="150" not-null="false"/>
  </class>
</hibernate-mapping>