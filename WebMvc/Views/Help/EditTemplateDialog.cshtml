@model Reports.Presenters.UI.ViewModel.HelpTemplateEditModel
<html>
<head>
    <title/>
   
</head>
<body>
    <table border="0" cellpadding="0" cellspacing="0" id = "TemplateDialogTable" style="width:100%;height:100px">
                <tr>
                    <td colspan="3">
                        <div id="TemplateError" style="color:Red;display:none;"/>
                        @Html.HiddenFor(model => model.Id)
                    </td>
                </tr>
                <tr>
                    <td colspan="3">
                        Перед прикреплением файла необходиимо ввести название
                    </td>
                </tr>
                <tr>
                    <td style="text-align:right;width:70px">@Html.LabelFor(model => model.Name):</td>
                    <td>
                       @Html.TextBoxFor(model => model.Name, (object)new { style = "width:100%" })
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <div id="file-uploader">
                            <noscript>
                                <p>Please enable JavaScript to use file uploader.</p>
                                <!-- or put a simple form for upload here -->
                            </noscript>
                        </div>
                    </td>
                </tr>
    </table>
</body>
</html>
<script type="text/javascript">
 
</script>
